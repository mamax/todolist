<%= form_for(@project) do |f| %>
   <%= render 'projects/error_messages'  %>
    <div class="form-group">
    <%= f.text_field :content, class: "form-control", placeholder: "Enter project name..."  %>
  </div>
    <div class="actions pull-right">
      <%= link_to 'Close', root_path, class: "btn btn-default" %>
      <%= f.submit "Ok", class: "btn btn-primary" %>
    </div>

<% end %>
